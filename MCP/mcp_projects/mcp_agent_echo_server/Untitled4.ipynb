    "proc.terminate()\n",
    "try:\n",
    "    proc.wait(timeout=5)\n",
    "    print(\"Flask server terminated.\")\n",
    "except subprocess.TimeoutExpired:\n",
    "    proc.kill()\n",
    "    proc.wait(timeout=5)\n",
    "    print(\"Flask server killed.\")\n"
